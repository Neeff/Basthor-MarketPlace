<div id="<%= shop.id %>">
<div class="col-lg-4 col-md-6 mb-4">
        <div class="card h-100">
          <a href="<%= shop_path(shop)%>"><%= image_tag(shop.images.first.variant(resize: '1000x800'),class: 'card-img-top') %></a>
          <div class="card-body">
            <h4 class="card-title">
              <a href="#"><%= shop.name %></a>
            </h4>
            <p class="card-text"><%= shop.description %></p>
          </div>
          <div class="card-footer">
            <small class="text-muted">&#9733; &#9733; &#9733; &#9733; &#9734;</small>
          </div>
        </div>
      </div>
</div>